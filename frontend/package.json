  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.dex.auditor",
    "files": [
      "build/**/*",
      "main.js",
      "../cli/**/*",
      "../tools/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "portable",
      "compression": "maximum"
    },
    "extends": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
